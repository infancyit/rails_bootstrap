<div class="jumbotron">
  <h3 class="display-3 text-center"><b>Show</b></h3>
  <p class="lead">
    <div class="text-center">
      <h2>First Name: <%= @person.first_name %></h2>
      <h2>Last Name: <%= @person.last_name %></h2>
      <h2>Email: <%= @person.email %></h2>
      <h2>Country: <%= @person.country_field %></h2>
      <h2>State: <%= @person.state_field %></h2>
      <a  href="/people/<%= @person.id %>/edit", class="btn btn-info">Edit</a>
      <a  href="/people/<%= @person.id %>/proceed", class="btn btn-info">Proceed</a>
    </div>
  </p>
</div>
